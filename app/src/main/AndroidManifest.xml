<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yev.pollinator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CreatePoll">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RunningPoll" />
        <activity
            android:name=".VoterRegistration"
            android:label="Vote Registration" />
        <activity
            android:name=".RunningPoolResults"
            android:label="@string/title_activity_running_pool_results"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ShowUsers" android:label="User Details"></activity>
    </application>

</manifest>